#include <stdio.h>
#define MAXSIZE 20
typedef int ElemType;
typedef struct
{
	ElemType data[MAXSIZE];
	int length;
	//length表示线性表当前的长度
}Sqlist;
//线性表的长度，数组的长度
//线性表长度是数据元素的个数
//数组的长度是指线性表实际占据的存储空间
int main()
{
   Sqlist num;
	InitList(&num);
   return 0;
}
//线性表的创建和初始化\
void InitList(Sqlist* L)
{
	
	L->length=0;
	for(int i=0;i<MAXSIZE/2;i++)
	{
		L->data[i]=i;
		L->length++:
	}
}

//判断线性表是否为空
void ListEmpty(Sqlist*L)
{
	if(0==L->Length)
		printf("it is null");
}
//将数据表清空
void ClearList(Sqlist*L)
{
		L->length=0;
	for(int i=0;i<MAXSIZE/2;i++)
	{
		L->data[i]=0;
	
	}
}
void GetElem(Sqlist*L,int i;int *e)
{
	*e=L->data[i];
}
void ListInsert(int *L,int i,int e)
{
	
}
void ListDelete(int*L,int i,int *e)
{
}
int ListLength(int *L)
{
}

